@model IEnumerable<OsAccountingApp1.Models.cost>

@{
    ViewBag.Title = "Цены ОС";
}

<h2>@ViewBag.Title</h2>

<div class="row">
    <label class="control-label col-md-2" for="id_os">Выбор ОС</label>
    <div class="col-md-10">
        @Html.DropDownList("id_os", null, htmlAttributes: new { @class = "form-control", @onclick = "getReportOS(this.value)" })
    </div>
</div>
<div class="row">
    <label class="control-label col-md-2" for="os_name">Наименование ОС</label>
    <div class="col-md-4">
        <p class="form-control" id="os_name">@ViewBag.os_name</p>
    </div>

</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.costchangedate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cost1)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.costchangedate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cost1)
            </td>
        </tr>
    }

</table>


